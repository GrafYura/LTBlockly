Blockly.Blocks['new_block_creator'] = {
    init: function () {
        this.appendDummyInput('qwe')
            .appendField("qweasd");
        //createBlocks();
    }
};

Blockly.JavaScript['new_block_creator'] = function (block) {
    // Search the text for a substring.
    var code = {
        before: "",
        gencode: "",
        after: "",
        global: ""
    };

    return JSON.stringify(code);
};















